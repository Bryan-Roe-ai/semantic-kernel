{
    "version": "0.2.0",
    "configurations": [
    {
        "name": "Python: Pytest",
        "type": "debugpy",
        "request": "launch",
        "module": "pytest",
        "args": ["${file}"],
        "console": "integratedTerminal",
        "justMyCode": true,
        "cwd": "${workspaceFolder}",
        "python": "${command:python.interpreterPath}"
    },
    {
      "name": ".NET Core Launch (dotnet-kernel-syntax-examples)",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build (KernelSyntaxExamples)",
      "program": "${workspaceFolder}/dotnet/samples/KernelSyntaxExamples/bin/Debug/net6.0/KernelSyntaxExamples.dll",
      "args": [],
      "cwd": "${workspaceFolder}/dotnet/samples/KernelSyntaxExamples",
      "stopAtEntry": false,
      "console": "integratedTerminal"
    },
    {
      "name": "CAPs - Demo Sample",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build (CopilotAgentPluginsDemoSample)",
      "program": "${workspaceFolder}/dotnet/samples/Demos/CopilotAgentPlugins/CopilotAgentPluginsDemoSample/bin/Debug/net8.0/CopilotAgentPluginsDemoSample.exe",
      "args": ["demo"],
      "cwd": "${workspaceFolder}/dotnet/samples/Demos/CopilotAgentPlugins/CopilotAgentPluginsDemoSample",
      "stopAtEntry": false,
      "console": "integratedTerminal"
    },
    {
      "name": "C#: Concept Samples",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build (Concepts)",
      "program": "${workspaceFolder}/dotnet/samples/Concepts/bin/Debug/net6.0/Concepts.dll",
      "args": [],
      "cwd": "${workspaceFolder}/dotnet/samples/Concepts",
      "console": "internalConsole",
      "stopAtEntry": false
    },
    {
      "name": "Attach to .NET Functions",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:azureFunctions.pickProcess}"
    },
    {
      "name": "RobotCode: Default",
      "type": "robotcode",
      "request": "launch",
      "purpose": "default"
    },
    {
      "name": "Python: Pytest",
      "type": "debugpy",
      "request": "launch",
      "module": "pytest",
      "args": ["${file}"],
      "console": "integratedTerminal",
      "justMyCode": true,
      "cwd": "${workspaceFolder}",
      "python": "${command:python.interpreterPath}"
    },
    {
      "name": "C#: HuggingFace ImageToText",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build (HuggingFaceImageToText)",
      "program": "${workspaceFolder}/dotnet/samples/Demos/HuggingFaceImageToText/bin/Debug/net8.0/HuggingFaceImageToText.dll",
      "args": [],
      "cwd": "${workspaceFolder}/dotnet/samples/Demos/HuggingFaceImageToText",
      "stopAtEntry": false,
      "console": "integratedTerminal"
    },
    {
      "name": "Python: FastAPI",
      "type": "debugpy",
      "args": [
        "${file}"
      ]
    },
    {
      "name": "C#: HuggingFaceImageToText Demo",
      "type": "dotnet",
      "request": "launch",
      "module": "uvicorn",
      "args": ["app:app", "--host", "0.0.0.0", "--port", "8000", "--reload"],
      "console": "integratedTerminal",
      "justMyCode": true,
      "cwd": "${workspaceFolder}"
    },
    {
      "name": "C#: GettingStarted Samples",
      "type": "coreclr",
      "request": "launch",
      "preLaunchTask": "build (GettingStarted)",
      "program": "${workspaceFolder}/dotnet/samples/GettingStarted/bin/Debug/net8.0/GettingStarted.dll",
      "args": [],
      "cwd": "${workspaceFolder}/dotnet/samples/GettingStarted",
      "stopAtEntry": false,
      "console": "integratedTerminal"
    },
        {
            "name": "C#: Process with Dapr",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "daprd-debug",
            "program": "${workspaceFolder}/dotnet/samples/ProcessWithDapr/bin/Debug/net8.0/ProcessWithDapr.dll",
            "env": {
                "DAPR_HTTP_PORT": "3500",
                "DAPR_GRPC_PORT": "50001"
            }
        },
        {
            "name": "C/C++ Runner: Debug Session",
            "type": "cppdbg",
            "request": "launch",
            "args": [],
            "stopAtEntry": false,
            "externalConsole": true,
            "cwd": "c:/Users/Bryan/OneDrive/SK",
            "program": "c:/Users/Bryan/OneDrive/SK/build/Debug/outDebug",
            "MIMode": "gdb",
            "miDebuggerPath": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
    {
      "name": "C#: Process with Dapr",
      "type": "coreclr",
      "request": "launch",
      "program": "${workspaceFolder}\\dotnet\\samples\\Demos\\ProcessWithDapr\\bin\\Debug\\net8.0\\ProcessWithDapr.dll",
      "preLaunchTask": "daprd-debug",
      "postDebugTask": "dapr-down",
      "env": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  ]
}