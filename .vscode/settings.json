  "terminal.integrated.env.windows": {
    "JAVA_HOME": "c:\\Program Files\\Eclipse Adoptium\\jdk-21.0.5.11-hotspot",
    "PATH": "c:\\Program Files\\Eclipse Adoptium\\jdk-21.0.5.11-hotspot\\bin;${env:PATH}"
  },
  "dbt.dbtIntegration": "cloud",
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[git-commit]": {
    "editor.defaultFormatter": "Google.cros-ide"
  },
  "maven.view": "flat",
  "java.jdt.ls.java.home": "/home/broe/semantic-kernel/.devcontainer/OpenJDK-23",
  "terminal.integrated.env.linux": {
    "JAVA_HOME": "/home/broe/semantic-kernel/.devcontainer/OpenJDK-23",
    "PATH": "/home/broe/semantic-kernel/.devcontainer/OpenJDK-23/bin:${env:PATH}"
  },
  "maven.terminal.customEnv": [
    {
      "environmentVariable": "JAVA_HOME",
      "value": "/home/broe/semantic-kernel/.devcontainer/OpenJDK-23"
    }
  ],
  "notebook.defaultFormatter": "GitHub.copilot",
  "databricks.sensitiveValueStore": "VSCodeSettings",
  "[code-text-binary]": {
