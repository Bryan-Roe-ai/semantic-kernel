{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "AzureOpenAISettings": {
    "ChatModelDeployment": "gpt-4o",
    "Endpoint": "https://api.openai.com/v1/engines/davinci-codex/completions",
    "ApiKey": "your-api-key"
  },
  "OpenAISettings": {
    "ChatModel": "gpt-4o",
    "ApiKey": "your-api-key"
  },
  "MongoDBSettings": {
    "ConnectionString": "mongodb://localhost:27017",
    "DatabaseName": "your-database-name",
    "DirectConnection": false
  },
  "AzureBlobStorageSettings": {
    "Endpoint": "https://your-storage-account.blob.core.windows.net"
  },
  "AzureCognitiveServicesSettings": {
    "Endpoint": "https://your-cognitive-services-endpoint",
    "ApiKey": "your-api-key"
  },
  "AzureFunctionsSettings": {
    "Endpoint": "https://your-functions-endpoint"
  },
  "CosmosDBSettings": {
    "Endpoint": "https://your-cosmosdb-endpoint"
  },
  "KeyVaultSettings": {
    "Endpoint": "https://your-keyvault-endpoint"
  },
  "AzureDevOpsSettings": {
    "OrganizationUrl": "https://dev.azure.com/your-organization"
  },
  "TaskGeneratorSettings": {
    "Endpoint": "https://your-task-generator-endpoint",
    "ApiKey": "your-api-key",
    "Topic": "your-topic",
    "DifficultyLevel": "your-difficulty-level",
    "TaskType": "your-task-type"
  },
  "GeneralSettings": {
    "ApiKey": "your-api-key",
    "DatabaseUrl": "https://your-database-url",
    "SecretKey": "your-secret-key"
  },
  "GitHubPagesSettings": {
    "Repository": "your_github_repository_here",
    "Branch": "main",
    "BuildCommand": "docker build -t my-app .",
    "RunCommand": "docker run -p 3000:3000 --env-file .env my-app",
    "DeployCommand": "actions/deploy-pages@v4"
  }
}
